  "version": "0.0.0",
  "homepage": "https://github.com/mdnorman/node-ts-react-relay-sapling-example-todo/wiki/Home",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdnorman/node-ts-react-relay-sapling-example-todo"
  },
  "bugs": {
    "url": "https://github.com/mdnorman/node-ts-react-relay-sapling-example-todo/issues"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf out",
    "build": "npm run relay && webpack --mode production",
    "build:development": "npm run relay && webpack --mode development",
    "prepare": "tsc",
    "pretest": "&& tsc -p spec/tsconfig.json && webpack --mode production",
    "test": "jasmine",
    "test:integration": "npm run pretest && jasmine --config=spec/support/jasmine-integration.json",
    "posttest": "npm run lint",
    "lint": "tslint -c tslint.json --project tsconfig.json && tslint -c tslint.json --project spec/tsconfig.json",
    "pretty": "pretty-quick --stage",
    "server:development": "node dev-server.js",
    "fetch-schema": "get-graphql-schema ${GRAPHQL_URL:-http://localhost:${GRAPHQL_PORT:-3000}/graphql} > ./schema.graphql",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx ts tsx",
    "relay:watch": "npm run relay -- --watch"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "graphql": "0.13.2",
    "node-sass": "4.9.3",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-relay": "1.6.2",
    "react-router-dom": "4.3.1",
    "relay-runtime": "1.6.2"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.8",
    "@types/node": "10.5.2",
    "@types/react-dom": "16.0.7",
    "@types/react-relay": "1.3.9",
    "@types/react-router-dom": "4.3.0",
    "@types/relay-runtime": "1.3.6",
    "awesome-typescript-loader": "5.2.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-relay": "1.6.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "1.0.0",
    "get-graphql-schema": "2.1.1",
    "html-webpack-plugin": "3.2.0",
    "jasmine": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "prettier": "1.13.7",
    "pretty-quick": "1.6.0",
    "relay-compiler": "1.6.2",
    "relay-compiler-language-typescript": "1.1.0",
    "relay-compiler-webpack-plugin": "0.11.0",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.3",
    "style-loader": "0.22.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.9.2",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2",
    "webpack": "4.16.5",
    "webpack-command": "0.4.1",
    "webpack-dev-server": "3.1.5"
  },
  "optionalDependencies": {},
  "engines": {
    "npm": ">=6.1.0",
    "node": ">=10.7.0"
  }
}
