{
  "author": "Michael D. Norman",
  "name": "node-ts-react-relay-example-todo",
  "description": "",
  "private": true,
  "license": "commercial",
  "version": "0.0.0",
  "homepage": "https://github.com/mdnorman/node-ts-react-relay-sapling-example-todo/wiki/Home",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdnorman/node-ts-react-relay-sapling-example-todo"
  },
  "bugs": {
    "url": "https://github.com/mdnorman/node-ts-react-relay-sapling-example-todo/issues"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf out",
    "build": "npm run relay && webpack --mode production",
    "build:development": "npm run relay && webpack --mode development",
    "rebuild": "npm run clean && npm run build",
    "rebuild:development": "npm run clean && npm run build:development",
    "pretest": "tsc -p spec/tsconfig.json && webpack --mode production",
    "test": "jasmine",
    "test:integration": "npm run pretest && jasmine --config=spec/support/jasmine-integration.json",
    "posttest": "npm run lint",
    "lint": "tslint -c tslint.json --project tsconfig.json && tslint -c tslint.json --project spec/tsconfig.json",
    "pretty": "pretty-quick --stage",
    "server:development": "node dev-server.js",
    "deploy": "serverless deploy -v --stage test && serverless client deploy -v --no-confirm --no-delete-contents --stage test",
    "deploy:prod": "serverless deploy -v --stage prod && serverless client deploy -v --no-confirm --no-delete-contents --stage prod",
    "fetch-schema": "get-graphql-schema ${GRAPHQL_URL:-http://localhost:${GRAPHQL_PORT:-3000}/graphql} > ./schema.graphql",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx ts tsx --language typescript",
    "relay:watch": "npm run relay -- --watch"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "graphql": "14.1.1",
    "node-sass": "4.11.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-relay": "2.0.0",
    "react-router-dom": "4.3.1",
    "relay-runtime": "2.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "3.3.8",
    "@types/node": "10.12.21",
    "@types/react-dom": "16.8.0",
    "@types/react-relay": "1.3.10",
    "@types/react-router-dom": "4.3.1",
    "@types/relay-runtime": "1.3.8",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "babel-plugin-relay": "2.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "2.1.0",
    "get-graphql-schema": "2.1.2",
    "html-webpack-plugin": "3.2.0",
    "jasmine": "3.3.1",
    "jasmine-spec-reporter": "4.2.1",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "relay-compiler": "2.0.0",
    "relay-compiler-language-typescript": "2.0.0",
    "relay-compiler-webpack-plugin": "2.0.1",
    "resolve-url-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "serverless": "1.37.0",
    "serverless-finch": "2.3.2",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.1",
    "url-loader": "1.1.2",
    "webpack": "4.29.2",
    "webpack-command": "0.4.2",
    "webpack-dev-server": "3.1.14"
  },
  "optionalDependencies": {},
  "engines": {
    "npm": ">=6.7",
    "node": ">=10.15"
  }
}
